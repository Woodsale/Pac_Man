public class map {

	private int[][] data;
	
	
	public map() {
		data = new int[120][120];
	}
	public map(int x, int y) {
		data = new int[x][y];
	}
	
	
	public int getWidth() {
		return data.length;
	}
	public int getHeight() {
		return (data[0]).length;
		
	}
	public int getArea() {
		return getWidth()*getHeight();
	}
	
	public Boolean set(int a, int b, int c) {
		try {
			data[a][b] = c;
		}
		catch (ArrayIndexOutOfBoundsException e) {
			return false;
		}
		return true;
	}
	public int get(int a, int b) {
		try {
			return data[a][b];
		}
		catch (ArrayIndexOutOfBoundsException e) {
			return 0;
		}
	}
	
	public String printMap() {
		String result = "";
		for (int i = 0;i<getWidth();i++) {
			for (int j = 0;j<getHeight();j++) {
				result += Integer.toString(get(i,j));
			}
			result += "\n";
		}
		return result;
		
	}
	
	
	
	
	public static void main(String args[]) {
		//testing
		map test = new map();
		
		System.out.println(Integer.toString(test.getArea()));
	}
	
}
