	public board() {
		for(int i = 0; i < board1temp.length; i++) {
			for(int j = 0; j < board1temp[0].length; j++) {
				Location loc = Location(i,j);
				if(board1temp[i][j] == 0) {
					WALL wall = new WALL(loc);
					board1[i][j] = wall;
				}
				else if(board1temp[i][j] == 1) {
					DOT pacdot = new DOT(loc);
					board1[i][j] = pacdot;
				}
				else if(board1temp[i][j] == 2) {
					board1[i][j] = null;
				}
			}
		}
		boards.add(board1);
		boards.add(board2);
